<Window x:Class="MySkype.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MySkype"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Start" x:Name="StartButton" HorizontalAlignment="Left" Margin="243,352,0,0" VerticalAlignment="Top" Width="75" Click="StartButton_Click"/>
        <Button Content="Stop" x:Name="StopButton" HorizontalAlignment="Left" Margin="473,352,0,0" VerticalAlignment="Top" Width="75" Click="StopButton_Click"/>
        <Label Content="Closed" x:Name="StatusLabel" HorizontalAlignment="Left" Margin="323,375,0,0" VerticalAlignment="Top" Height="12" Width="134"/>
        <TextBox x:Name="IpInput" HorizontalAlignment="Left" Height="23" Margin="336,352,0,0" TextWrapping="Wrap" Text="127.0.0.1" VerticalAlignment="Top" Width="120"/>
        <Image x:Name="MyImage" HorizontalAlignment="Left" Height="347" VerticalAlignment="Top" Width="371" Margin="204,0,0,0"/>

    </Grid>

</Window>
